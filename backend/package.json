{
  "name": "cloud-deploy-portal-backend",
  "version": "3.0.0",
  "description": "Backend API for Cloud Auto Deploy Portal with 20 cloud services",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "db:start": "docker start cloud-portal-db cloud-portal-redis 2>/dev/null || docker compose -f ../docker-compose.yml up -d postgres redis",
    "db:stop": "docker stop cloud-portal-db cloud-portal-redis",
    "predev": "npm run db:start",
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.500.0",
    "@aws-sdk/client-dynamodb": "^3.500.0",
    "@aws-sdk/client-ec2": "^3.500.0",
    "@aws-sdk/client-ecs": "^3.500.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.975.0",
    "@aws-sdk/client-elasticache": "^3.975.0",
    "@aws-sdk/client-iam": "^3.975.0",
    "@aws-sdk/client-lambda": "^3.500.0",
    "@aws-sdk/client-rds": "^3.500.0",
    "@aws-sdk/client-route-53": "^3.975.0",
    "@aws-sdk/client-s3": "^3.500.0",
    "@aws-sdk/client-sns": "^3.500.0",
    "@aws-sdk/client-sqs": "^3.500.0",
    "@aws-sdk/client-ssm": "^3.975.0",
    "@aws-sdk/client-sts": "^3.500.0",
    "@azure/arm-appservice": "^14.0.0",
    "@azure/arm-cdn": "^9.0.0",
    "@azure/arm-compute": "^21.0.0",
    "@azure/arm-containerinstance": "^9.0.0",
    "@azure/arm-cosmosdb": "^16.0.0",
    "@azure/arm-dns": "^5.1.0",
    "@azure/arm-network": "^33.5.0",
    "@azure/arm-resources": "^5.0.0",
    "@azure/arm-servicebus": "^30.0.0-beta.1",
    "@azure/arm-sql": "^10.0.0",
    "@azure/arm-storage": "^18.0.0",
    "@azure/identity": "^4.0.0",
    "@google-cloud/compute": "^6.7.0",
    "@google-cloud/network-management": "^5.1.1",
    "@google-cloud/storage": "^7.18.0",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.0",
    "pg": "^8.11.0",
    "uuid": "^9.0.0",
    "winston": "^3.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@vitest/ui": "^4.0.18",
    "vitest": "^4.0.18"
  }
}
